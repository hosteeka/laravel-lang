{
    "name": "laravel-lang",
    "version": "0.1.1",
    "description": "Use your Laravel translations in JavaScript.",
    "keywords": [
        "laravel",
        "localization",
        "language",
        "translation",
        "javascript"
    ],
    "homepage": "https://github.com/hosteeka/laravel-lang/packages/langjs",
    "bugs": "https://github.com/hosteeka/laravel-lang/issues",
    "license": "MIT",
    "author": "Melvin Otieno <o.melvinotieno@gmail.com> (https://melvinotieno.com)",
    "files": [
        "dist",
        "types"
    ],
    "type": "module",
    "source": "./src/index.js",
    "exports": {
        "default": "./dist/index.js",
        "types": "./types/index.d.ts"
    },
    "module": "./dist/index.esm.js",
    "types": "./types/index.d.ts",
    "scripts": {
        "dev": "microbundle watch -i ./src/index.js -o ./dist/index.js --format modern,esm --no-sourcemap --no-generateTypes",
        "build": "microbundle -i ./src/index.js -o ./dist/index.js --format modern,esm --no-sourcemap --no-generateTypes"
    },
    "devDependencies": {
        "microbundle": "^0.15.1"
    }
}
